reset

set output '{{ filenamePcaPng }}'
set terminal png size 1024,768
NO_ANIMATION = 1

#set title 'PCA Explained Variance'
#set ylabel 'Eigenvector'
#set xlabel 'Explained Variance [AU]'
set title ''
set ylabel ''
set xlabel ''
set xrange [{{ range }}] noreverse nowriteback
set yrange [0.0:*] noreverse nowriteback

unset key
set grid

set style line 1 lt 1 lc rgb '#aa0000'
set style line 2 lt 1 lc rgb '#00aa00'
set style line 3 lt 1 lc rgb '#0000aa'

set boxwidth 0.9
set style fill solid 1.00 border 0
set style histogram {{ errorbars }}
set style data histograms
set bars 0.5

plot '{{ filenamePca }}' using {{ using }} ti '' ls 3

set terminal tikz plotsize 9,6
set output '{{ filenamePcaTex }}'
replot



set terminal tikz plotsize 9,6
set output '{{ filenamePcaTex }}'
replot
